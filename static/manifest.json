{
  "name": "Web Wizard",
  "description": "A web wizard for creating web apps",
  "version": "0.0.1",
  "manifest_version": 3,
  "web_accessible_resources": [
    {
      "resources": ["contentScript.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
